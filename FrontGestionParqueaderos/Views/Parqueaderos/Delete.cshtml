@model FrontGestionParqueaderos.Models.Parqueadero
@{
    ViewData["Title"] = "Parqueaderos";
}
<div class="container p-3 my-3 border" >
    <h4>¿Está Seguro que desea Eliminar este registro?</h4>

    <div class="row">
        <div class="col-sm-6">
            <hr />
            <form asp-action="Delete">
                <div class="form-group">
                    <input type="hidden" asp-for="Nomenclatura" class="form-control" />
                </div>
                <br />
                <div class="form-group">
                    <a class="btn btn-outline-primary" asp-action="Index">Regresar</a> |
                    <a class="btn btn-outline-danger" asp-action="Eliminar" asp-route-Nomenclatura ="@Model.Nomenclatura">Eliminar</a>
                </div>
            </form>
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}