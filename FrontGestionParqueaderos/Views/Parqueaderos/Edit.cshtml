@model FrontGestionParqueaderos.Models.Parqueadero
@{
    ViewData["Title"] = "Parqueaderos";
}
<div class="container p-3 my-3 border" >
    <h1>Editar</h1>

    <div class="row">
        <div class="col-sm-6">
            <hr />
            <form asp-action="Edit">
                <div class="form-group">
                    <b><label asp-for="Nomenclatura" class="control-label"></label></b>
                    <input asp-for="Nomenclatura" class="form-control" />
                    <span asp-validation-for="Nomenclatura" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <b><label asp-for="Piso" class="control-label"></label></b>
                    <input asp-for="Piso" class="form-control" />
                    <span asp-validation-for="Piso" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <b><label asp-for="Estado" class="control-label"></label></b>
                    <select asp-for="Estado" class="form-control" asp-items="ViewBag.Estados"></select>
                    <span asp-validation-for="Estado" class="text-danger"></span>
                </div>
                <br />
                <div class="form-group">
                    <input class="btn btn-success" type="submit" value="Guardar" />
                    <a class="btn btn-primary" asp-action="Index">Regresar</a>
                </div>
            </form>
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}